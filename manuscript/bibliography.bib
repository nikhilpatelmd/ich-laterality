  author = {Moullaali, Tom J. and Wang, Xia and Martin, Rene{\'e} H. and Shipes, Virginia B. and Robinson, Thompson G. and Chalmers, John and Suarez, Jose I. and Qureshi, Adnan I. and Palesch, Yuko Y. and Anderson, Craig S.},
  year = {2019},
