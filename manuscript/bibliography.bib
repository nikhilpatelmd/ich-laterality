@article{tleyjehOverlookedShortcomingsObservational2021,
  title = {Overlooked {{Shortcomings}} of {{Observational Studies}} of {{Interventions}} in {{Coronavirus Disease}} 2019: {{An Illustrated Review}} for the {{Clinician}}},
