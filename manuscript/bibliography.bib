@article{boschAtrialFibrillationICU2018,
  title = {Atrial {{Fibrillation}} in the {{ICU}}},
  author = {Bosch, Nicholas A. and Cimini, Jonathan and Walkey, Allan J.},
  year = {2018},
  month = dec,
  journal = {Chest},
