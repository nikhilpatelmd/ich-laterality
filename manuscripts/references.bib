  title = {Confounding in the {{Association}} of {{Proton Pump Inhibitor Use With Risk}} of {{Community-Acquired Pneumonia}}},
  author = {Jena, Anupam B. and Sun, Eric and Goldman, Dana P.},
  year = {2013},
