@article{hebbelstrupryerasmussenEventdrivenNatureOnline2023,
  title = {The Event-Driven Nature of Online Political Hostility: {{How}} Offline Political Events Make Online Interactions More Hostile},
  shorttitle = {The Event-Driven Nature of Online Political Hostility},
  author = {Hebbelstrup Rye Rasmussen, Stig and Petersen, Michael Bang},
  editor = {Contractor, Noshir},
  year = {2023},
