  langid = {english},
  keywords = {_tablet,bayesian,ordinal},
  file = {/home/nikhil/Zotero/storage/BRV6P4C3/Carmona-Bayonas et al_2021_Prediction of quality of life in early breast cancer upon completion of.pdf}
}

