  author = {Moura, Lidia M.V.R. and Donahue, Maria A. and Yan, Zhiyu and Smith, Louisa H. and Hsu, John and Newhouse, Joseph P. and Schwamm, Lee H. and Haneuse, Sebastien and {Hernandez-Diaz}, Sonia and Blacker, Deborah},
  year = {2023},
